[filter:cors]
use = egg:wsgicors#middleware
policy = open
open_origin = *
open_headers = *
open_expose_headers = *
open_methods = *
open_maxage = 86400

[app:versions_app]
paste.app_factory = versionsapp:factory

[app:version_1_app]
paste.app_factory = apiv1app:factory

[pipeline:versions_api]
pipeline = cors versions_app

[pipeline:version_1_api]
pipeline = cors version_1_app

[composite:main]
use=egg:Paste#urlmap
/ = versions_api
/v1 = version_1_api

[server:main]
use = egg:Paste#http
host = 10.0.7.117
port = 9494
